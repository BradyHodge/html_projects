header      {text-align: center;
            height: 175px;
            margin: auto;
            line-height: 88px;}

body        {background-image: url(babbage-header.jpg);
            background-repeat: repeat-x;
            color: #fcfcfc}

section     {position: absolute;
            top: 200px;}

h1          {font-size: 40px;
            color: #000000;
            background-color: rgba(255, 255, 255, 0.5);
            border-radius: 10px;}

h2          {font-size: 25px;}

article      {text-align: left;
              color: #000000;
              padding: 15px;}

a: visited    {color: #0000ff;}

a           {margin: auto;
             text-decoration: none;
              color: #0000ff;
              display: block;
              background-color: rgb(0, 0, 0, 0.15);
              width: 100px;
              padding: 8px;
              border-radius: 5px;
              color: #0000ff;}
			  
nav			{text-align: center;}

img           {margin: 5px;
              border: solid #000000 1px;
              border-radius: 5px;}

ul            {list-style-type: circle;
              border: solid #000000 1px;
              border-radius: 5px;
              padding-top: 5px;
              padding-bottom: 5px;
              padding-right: 5px;}